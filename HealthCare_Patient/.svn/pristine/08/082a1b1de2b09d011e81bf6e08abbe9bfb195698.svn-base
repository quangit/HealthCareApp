<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_checked="true"
      
      android:state_focused="false">
    <shape>

      <solid android:color="#2CBE71" >
      </solid>

      <stroke
          android:width="2dp"
          android:color="#2CBE71" >
      </stroke>
      <corners android:radius="5dp" />
    </shape>
  </item>

  <item android:state_checked="true"
      android:state_focused="true">
    <shape>

      <solid android:color="#2CBE71" >
      </solid>

      <stroke
          android:width="2dp"
          android:color="#2CBE71" >
      </stroke>
      <corners android:radius="5dp" />
    </shape>

  </item>

  <item android:state_checked="false"
      android:state_focused="false">
    <shape>

      <solid android:color="#2CBE71" >
      </solid>

      <stroke
          android:width="2dp"
          android:color="#2CBE71" >
      </stroke>
      <corners android:radius="5dp" />
    </shape>
  </item>

  <item android:state_checked="false"
      android:state_focused="true">
    <shape>

      <solid android:color="#2CBE71" >
      </solid>

      <stroke
          android:width="2dp"
          android:color="#2CBE71" >
      </stroke>
      <corners android:radius="5dp" />
    </shape>
  </item>
</selector>